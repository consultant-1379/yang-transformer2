      choice snmp-traps {
        description
          "";
        case all {
          leaf all {
            type empty;
            description
              "Enable sending all supported OSPF traps";
          }
        }
        case snmp-traps-choice {
          leaf snmp-traps-choice {
            type bits {
              bit nbrstatechange {
                position 0;
                description
                  "Enable sending ospf3NbrStateChange traps";
              }
              bit ifstatechange {
                position 1;
                description
                  "Enable sending ospf3ifStateChange traps";
              }
            }
            description
              "leaf snmp-traps-choice.";
          }
        }
      }
    }
    /*
