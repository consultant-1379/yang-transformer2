    /*
    hello-interval (interval-spec)
    */
    leaf hello-interval {
      type uint16 {
        range "1..65535";
      }
      default "10";
      description
        "Configure interface hello interval";
    }
    /*
    router-dead-interval (interval-spec)
    */
    leaf router-dead-interval {
      type uint16 {
        range "1..65535";
      }
      description
        "Configure interface router dead interval";
    }
    /*
    authentication hmac-cryptographic (key-chain) [ transition-
     mode ]
    */
    container authentication-hmac-cryptographic {
      presence "";
      description
        "Cryptographic Hashed Message Code Autenthentication
         (HMAC)";
      leaf key-chain {
        type string;
        mandatory true;
        description
          "Key-chain identifier";
      }
      leaf transition-mode {
        type empty;
        description
          "Accept both authenticated and unauthenticated
           packets";
      }
    }
  }

  augment "/ctxsipos:contexts/ctxipos:context/ctxipos:router" {
    description
      "ericsson-ospf3";
    /*
    ospf3 (instance-id)
    */
    list ospf3 {
      key "ospf3";
      description
        "Open Shortest Path First (OSPFv3)";
      leaf ospf3 {
        type uint16 {
          range "1..65535";
        }
        description
          "OSPFv3 Instance Identifier";
      }
      uses context_router_ospf3-grp;
    }
  }

  augment "/ctxsipos:contexts/ctxipos:context" {
    description
      "ericsson-ospf3";
    /*
    ospf3 { high-res-timers | drop-non-link-local | sched-delay
     (delay-usecs) }
    */
    container ospf3 {
      when "../ctxipos:context-name='local'" {
        description
          "";
      }
      description
        "Configure OSPFv3 local context parameters";
      leaf high-res-timers {
        when "../../ctxipos:context-name='local'" {
          description
            "";
        }
        type boolean;
        default "true";
        description
          "Enable/Disable high-res-timers";
      }
      leaf drop-non-link-local {
        when "../../ctxipos:context-name='local'" {
          description
            "";
        }
        type boolean;
        default "true";
        description
          "Enable/Disable drop-non-link-local";
      }
      leaf sched-delay {
        when "../../ctxipos:context-name='local'" {
          description
            "";
        }
        type uint32 {
          range "1..200000";
        }
        description
          "Configure scheduler inter-event delay (usecs)";
      }
    }
  }

